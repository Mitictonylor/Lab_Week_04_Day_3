<div class= "form-wrapper">
<form class="form" action='/students/<%=@students.id%>' method="POST">
<div class="form-element">
  <label for="first_name" class="first">First name:</label>
<input type="text" name="first_name" id="first_name" value="<%= @students.first_name %>"/>
</div>
<div class="form-element">
    <label for="second_name" class="first">Second name:</label>
    <input id="second_name" type="text" name="second_name"value="<%= @students.second_name %>" />
</div>
<div class="form-element">
  <label for="age" class="age">Age:</label>
    <input id="age" type="number" name="age" value="<%= @students.age %>"/>
</div>
<div class="form-element">
  <label for="House" class="house">House:</label>
  <select id="house" name="house_id">
    <% for house in @houses %>
      <option value="<%= house.id %>">
          <%=  house.name() %>
      </option>
    <% end %>
  </select>
</div>
<button class="form-button" type="submit" name="button">APPLY CHANGES</button>
</form>
</div>
